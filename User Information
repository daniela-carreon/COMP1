#include<iostream>
using namespace std;
int main()
{
	//Obtain the ff information from the user and display in a resume format.
	//  name, age, address, phone number, school, and department.
	
	
	string name, address, school, dept;
	int age, numb;
	
	cout<<"Type your Name: ";
	cin>>name;
	
	cout<<"Type your age: ";
	cin>>age;
	
	cout<<"Type your address: ";
	cin>>address;
	
	cout<<"Type your phone number: ";
	cin>>numb;
	
	cout<<"Type your school: ";
	cin>>school;
	
	cout<<"Type your department: ";
	cin>>dept;
	
	cout<<"\n\n\t\tProfile";
	cout<<"\nName: "<<name;
	cout<<"\nAge: "<<age;
	cout<<"\nAddress: "<<address;
	cout<<"\nPhone Number: "<<numb;
	cout<<"\nSchool: "<<school;
	cout<<"\nDepartment: "<<dept;
	



	return 0;
	
}
